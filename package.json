{
  "name": "react-locales-detector",
  "version": "0.3.1",
  "description": "React locales detector",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint ./src ./tests --ext .ts --ext .tsx",
