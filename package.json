{
  "name": "react-locales-detector",
  "version": "0.1.0",
  "description": "React locales detector",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx ./src ./tests",
    "lint-export": "npm run lint -- -o ./tests_results/checkstyle/js-checkstyle.xml -f checkstyle",
    "flow": "flow",
    "flow-init": "flow-typed install jest@22.x enzyme@3.x enzyme-adapter-react-16@x.x query-string@6.x",
    "flow-coverage": "flow-coverage-report -f \"node node_modules/flow-bin/vendor/flow\" -i ./src/**/*.js* -t html -t text -t json -o tests_results/coverage/flow",
    "develop": "webpack-dev-server --mode development --content-base build/",
    "build": "rm -rf ./dist && rollup --config ./rollup.config.js",
    "test": "jest",
    "test-build": "npm run test -- --config jest.config.build.js",
    "prepublishOnly": "npm run build && npm run test-build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fabulator/react-locales-detector.git"
  },
  "author": "Michal Ozog√°n <michal@ozogan.eu>",
  "license": "Apache License 2.0",
  "homepage": "https://github.com/fabulator/react-locales-detector#readme",
  "dependencies": {
    "locales-detector": "^1.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@socifi/eslint-config": "^1.10.0",
    "@socifi/jest-config": "^1.9.0",
    "@socifi/rollup-config": "^1.10.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "flow-bin": "^0.78.0",
    "flow-coverage-report": "^0.5.0",
    "flow-typed": "^2.5.1"
  }
}
